{
  "name": "one-army-functions",
  "version": "1.0.0",
  "scripts": {
    "lint": "tslint --project tsconfig.json",
    "build": "tsc",
    "copy:local": "cp ./src/Firebase/admin.local.ts ./src/Firebase/admin.ts",
    "copy:server": "cp ./src/Firebase/admin.server.ts ./src/Firebase/admin.ts",
    "serve": "npm run build && firebase serve --only functions",
    "shell": "npm run build && firebase functions:shell",
    "deploy": "npm run copy:local && firebase use default && firebase deploy --only functions",
    "start": "npm run shell",
    "logs": "firebase functions:log"
  },
  "main": "lib/index.js",
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "child-process-promise": "^2.2.1",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "firebase-admin": "^6.2.0",
    "firebase-functions": "^2.1.0",
    "google-auth-library": "^2.0.1",
    "googleapis": "^35.0.0",
    "log-update": "^3.2.0"
  },
  "devDependencies": {
    "tslint": "~5.8.0",
    "typescript": "3.1.6"
  },
  "engines": {
    "node": "8"
  },
  "private": true
}
